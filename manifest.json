{
    "name": "Auto accept google meet",
    "manifest_version": 3,
    "version": "0.0.1",
    "description": "Extens√£o para clicar em confirmar no meet automaticamente.",
    "author": "Italo Oliveira",
    "action": {
      "default_icon":"icon.png",
      "default_title": "Auto accept google meet",
      "icons": {
        "128": "icon.png"
      }
    },
    "content_scripts": [{
      "matches": [ "*://meet.google.com/*" ],
      "js": ["click.js"]
    }],
    "permissions": [
      "tabs"     
    ]
}